<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="com.tm.pro.bean.MetrochennaiCost" table="metro_chennai_cost">
      <composite-id>
      <key-property name="pickup" column="pickup_place" />
      <key-property name="drop" column="destination"/>
    </composite-id>
    <property column="cost" name="cost"/>
  </class>
</hibernate-mapping>

